	$Name.text = achieve.get_achieve_name()
	$ProgressBar.max_value = achieve.get_amount()
	amount_to_achieve = achieve.get_amount()
	$ProgressBar/Cant.text = \"0/\"+str(amount_to_achieve)
	pass"
