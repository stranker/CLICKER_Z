# Seek TYPE
const TYPE_SURVIVOR = 0
const TYPE_WEAPON = 1
const TYPE_SUPPLY = 2

func _ready():
	pass

func set_info(typ,result):
	var label = \"\"
	match typ:
		TYPE_SURVIVOR:
			label = \"Survivor found!\"
		TYPE_WEAPON:
			label = \"Weapon found!\"
		TYPE_SUPPLY:
			label = \"Supplies found!\"
	if result:
		$Result.text = label
	else:
		$Result.text = \"Search failed\"
	pass

func _on_CloseRButton_pressed():
	queue_free()
	pass # replace with function body
